each file in files
    .album-cell
        img.album-cell__img(src=file.miniature, width='100%', height='100%')
        .album-cell__meta
            .album-cell__time #{file.data.time}
            .album-cell__f #{file.data.aperture}
            .album-cell__shotter #{file.data.exposure}
            .album-cell__iso #{file.data.iso}
        .album-cell__date #{file.data.date}
